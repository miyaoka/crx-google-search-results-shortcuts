{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "1.0.0",

  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },

  "browser_action": {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "include_globs": ["*://www.google.*/search?*"],
      "js": ["scripts/content.js"],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
